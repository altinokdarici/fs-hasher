{
  "name": "@fs-hasher/client",
  "version": "0.1.0",
  "description": "Node.js client for fs-hasher daemon",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "test": "node --test dist/**/*.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hash",
    "file",
    "watcher",
    "daemon",
    "ipc"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.19.1",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "optionalDependencies": {
    "@fs-hasher/darwin-arm64": "workspace:*",
    "@fs-hasher/darwin-x64": "workspace:*",
    "@fs-hasher/linux-x64-gnu": "workspace:*",
    "@fs-hasher/linux-x64-musl": "workspace:*",
    "@fs-hasher/win32-arm64-msvc": "workspace:*",
    "@fs-hasher/win32-x64-msvc": "workspace:*"
  }
}
