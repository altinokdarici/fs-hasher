{
  "name": "fswatchd",
  "version": "0.3.0",
  "description": "Node.js client for fswatchd daemon",
  "type": "module",
  "packageManager": "pnpm@10.23.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "test": "node --test dist/**/*.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hash",
    "file",
    "watcher",
    "daemon",
    "ipc"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/altinokdarici/fs-hasher.git",
    "directory": "client-js"
  },
  "homepage": "https://github.com/altinokdarici/fs-hasher#readme",
  "devDependencies": {
    "@types/node": "^22.19.1",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
